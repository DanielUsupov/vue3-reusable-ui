{
  "name": "vue3-ui",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vue-tsc": "^1.8.5"
  },
  "files": [
    "dist",
    "src/components/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielUsupov/vue3-reusable-ui.git"
  },
  "keywords": [
    "reusable-components",
    "vue3",
    "vue",
    "vuejs"
  ],
  "main": "./dist/vue3-reusable-ui.umd.cjs",
  "module": "./dist/vue3-reusable-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-reusable-ui.js",
      "require": "./dist/vue3-reusable-ui.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
